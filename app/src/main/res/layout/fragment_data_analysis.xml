<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/layout_fragment_title" />


    <TextView
        android:id="@+id/tv_power_analysis_data"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/shape_tv_count"
        android:gravity="center"
        android:text="系统用能" />

    <TextView
        android:id="@+id/tv_system_analysis_data"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/shape_tv_count"
        android:gravity="center"
        android:text="系统分析" />

    <TextView
        android:id="@+id/tv_count_analysis_data"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/shape_tv_count"
        android:gravity="center"
        android:text="电量电费分时统计" />


</LinearLayout>